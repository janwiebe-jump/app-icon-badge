{
  "name": "app-icon-badge",
  "license": "MIT",
  "version": "0.0.2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "assets"
  ],
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts && cp -r assets dist",
    "release": "pnpm run build && changeset publish",
    "start": "ts-node example/index.ts",
    "type-check": "tsc --noEmit",
    "preinstall": "npx only-allow pnpm",
    "np": "pnpm build && np --no-cleanup ",
    "test": "echo \"no test specified\"",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obytes/app-icon-badge.git"
  },
  "author": "Siham Ben Daia",
  "bugs": {
    "url": "https://github.com/obytes/app-icon-badge/issues"
  },
  "homepage": "https://github.com/obytes/app-icon-badge#readme",
  "dependencies": {
    "jimp": "^0.22.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.15.11",
    "husky": "^8.0.3",
    "np": "^7.7.0",
    "prettier": "2.8.7",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
